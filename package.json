{
  "name": "ui-tests-boilerplate",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "cypress:open:local": "CYPRESS_ENV=true cypress open",
    "cypress:open:staging": "CYPRESS_ENV=true cypress open --env configFile=staging",
    "cypress:open:production": "CYPRESS_ENV=true cypress open --env configFile=production",
    "cypress:test:local": "CYPRESS_ENV=true cypress run --spec **/integration/*.feature",
    "cypress:test:staging": "CYPRESS_ENV=true cypress run  --spec **/e2e/*.feature --env configFile=staging",
    "cypress:test:production": "CYPRESS_ENV=true cypress run --spec **/e2e/*.feature --env configFile=production",
    "cypress:test:jenkins": "CYPRESS_ENV=true cypress run --spec **/integration/*.feature --env configFile=jenkins",
    "cypress:report": "allure generate allure-results --clean -o allure-report && allure open allure-report"
  },
  "dependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "cypress": "4.2.0",
    "cypress-cucumber-preprocessor": "1.19.1",
    "eslint": "5.16.0",
    "mocha": "6.2.0",
    "mocha-allure-reporter": "1.4.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false
  }
}
